{
  "name": "site",
  "version": "0.0.5",
  "scripts": {
    "ng": "ng",
    "start": "docgeni serve",
    "build": "ng build",
    "build:prod": "docgeni build --prod",
    "pub": "yarn build:prod && cpx ./package.json ./dist/site && yarn pub:git",
    "pub:git": "wpm git-publish --source ./dist/site --name site-builds --organization ngnice",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "preinstall": "node scripts/check-yarn.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.2.16",
    "@angular/cdk": "^12.2.13",
    "@angular/common": "~12.2.16",
    "@angular/compiler": "~12.2.16",
    "@angular/core": "~12.2.16",
    "@angular/forms": "~12.2.16",
    "@angular/platform-browser": "~12.2.16",
    "@angular/platform-browser-dynamic": "~12.2.16",
    "@angular/router": "~12.2.16",
    "rxjs": "~6.5.4",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.13",
    "@angular/cli": "~12.2.13",
    "@angular/compiler-cli": "~12.2.16",
